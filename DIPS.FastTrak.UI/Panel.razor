<article class="Panel" @attributes="Attributes">
    <CascadingValue Value="this">
        @ChildContent
    </CascadingValue>
</article>

@code {
    [Parameter(CaptureUnmatchedValues = true)]
    public Dictionary<string, object>? Attributes { get; set; }

    [Parameter]
    public RenderFragment? ChildContent { get; set; }
}